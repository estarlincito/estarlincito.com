    "negotiator": "^1.0.0"
  },
  "devDependencies": {
    "@repo/next-config": "workspace:*",
    "@repo/types": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/negotiator": "^0.6.4"
  }
}
